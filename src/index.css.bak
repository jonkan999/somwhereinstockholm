
.map-container {
    position: absolute; 
    z-index: -1;
    top: 63vh;
    left: 0;
    width: 100vw;
    height: 48.5vh;

}



.street-container {
    position: absolute;
    left:0;
    top: 0;
    bottom: 0;
    height: 63vh;
    width: 100vw;
    padding: 0;
    margin: 0;

    -webkit-filter: saturate(1.4) contrast(1.1);
    filter: saturate(1.4) contrast(1.1);
}

.add-btn {
	position: absolute; 
	  background-color: granite;
  color: white;
    z-index: 1;
    top: 90vh;
    left: 40vw;
	width: 20vw;
	opacity: 50;
	cursor: pointer;
	
}


